.overlay 
	position absolute
	bottom 0
	left 0
	background rgba(0,118,192,0.75)
	width 100%
	height 0
	overflow hidden
	-webkit-transition 1s ease-out
	-moz-transition 1s ease-out
	-o-transition 1s ease-out
	-ms-transition 1s ease-out
	transition 1s ease-out

.overlay a 
	top 58%
	left 50%
	text-align center
	transform translate(-50%, -50%)
	position absolute

.overlay span 
	color #fff

.overlay h2 
	color #fff
	font-size 1.5em
	position absolute
	top 50%
	left 50%
	text-align center
	transform translate(-50%, -50%)
	text-transform uppercase
	-o-text-shadow 2px 2px 2px #553a55
	-ms-text-shadow 2px 2px 2px #553a55
	-moz-text-shadow 2px 2px 2px #553a55
	-webkit-text-shadow 2px 2px 2px #553a55
	text-shadow 2px 2px 2px #553a55

.imagenes:hover .overlay 
	height: 100%
	opacity: 0.9

.overlayRow 
	position absolute
	bottom 0
	left 0
	background #fff
	width 100%
	height 0
	overflow hidden
	-o-transition 0.3s ease
	-ms-transition 0.3s ease
	-moz-transition 0.3s ease
	-webkit-transition 0.3s ease
	transition 0.3s ease

.overlayRow a 
	top 58%
	left 50%
	text-align center
	transform translate(-50%, -50%)
	position absolute
	color #3f47bf

.overlayRow span 
	color #fff

.overlayRow h2 
	width 100%
	color #3a448b
	font-size 1em
	position absolute
	top 50%
	left 50%
	text-align center
	transform translate(-50%, -50%)
	text-transform uppercase
	font-family Arial

.rowOverhover .overlayRow 
	height 100%
	opacity 0.9

.rowOver 
 	position relative

